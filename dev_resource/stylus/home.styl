@import "./base/base";
header
	position fixed
	top 0
	width 100%
	height 50px
	background-color rgb(36, 36, 36)
	.icon
		position absolute
		top 5px
		&.back
			left @top
		&.option
			right @top
	h1
		margin-left 150px
		margin-right 150px
		overflow hidden
		text-align center
		white-space nowrap
		text-overflow ellipsis
		color white
		font-size 28px
		line-height 50px
body
	padding-top 50px
	font-size 40px