<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=320" />
		<title>点名系统V0.0.2</title>
	</head>
	<style>
		*{
			margin:0;
			padding:0;
			box-sizing:border-box;
		}
		.random{
			position:fixed;
			top:50%;
			left:50%;
			transform:translate(-50%, -50%);
			width:320px;
			height:568px;
			box-shadow:0 0 2px 1px rgb(204, 204, 204);
		}
		.page{
			position:absolute;
			top:0;
			left:0;
			width:100%;
			height:100%;
		}
		.page.loader{
			background-color:black;
			z-index:1;
		}
		.page.loader .number{
			margin-top:400px;
			text-align:center;
			color:white;
		}
		.page.loader .number:after{
			content:"％";
		}
		.page.loader .progress{
			margin:40px auto 0;
			width:280px;
			height:20px;
			border:1px solid white;
		}
		.page.loader .progress .inner{
			height:100%;
			background-color:white;
		}
		.btn{
			width:100px;
			height:30px;
			text-align:center;
			cursor:pointer;
		}
		.btn-bd{
			border-width:1px;
			border-style:solid;
			line-height:28px;
		}
		.btn-bg{
			line-height:30px;
		}
		.ipt-txt{
			padding:0 8px;
			width:160px;
			height:30px;
			line-height:28px;
		}
		.dialog{
			position:absolute;
			top:50%;
			left:50%;
			transform:translate(-50%, -50%);
			width:300px;
			height:540px;
			background-color:rgba(255, 255, 255, .9);
			box-shadow:0 0 2px 1px rgb(204, 204, 204);
		}
		.hidden{
			display:none;
		}
		.dialog h1{
			margin-top:10px;
			text-align:center;
			font-size:20px;
			line-height:2;
		}
		.welcome{
			background-image:url(data:image/jpg;base64,);
		}
		.welcome .title{
			margin-top:150px;
			text-align:center;
			font-size:30px;
			font-weight:bold;
			color:rgb(255, 0, 0);
			-webkit-text-stroke:.5px rgb(255, 64, 0);
		}
		.welcome .title:after{
			font-size:14px;
			content:"V0.0.2 beta";
		}
		.welcome .btn.start{
			position:absolute;
			top:320px;
			left:50%;
			transform:translateX(-50%);
			background-color:rgb(64, 201, 64);
			color:white;
		}
		.welcome .btn.setting{
			position:absolute;
			top:360px;
			left:50%;
			transform:translateX(-50%);
			background-color:rgb(128, 128, 128);
			color:white;
		}
		.welcome .btn.exit{
			position:absolute;
			top:400px;
			left:50%;
			transform:translateX(-50%);
			background-color:rgb(255, 64, 0);
			color:white;
		}
		.welcome .dialog.setting div{
			margin-top:10px;
			text-align:center;
		}
		.welcome .dialog.setting .sure{
			display:block;
			margin:300px auto 0;
			background-color:white;
			border-color:rgb(204, 204, 204);
			color:rgb(102, 102, 102);
		}
		.game .controller{
			position:absolute;
			bottom:0;
			padding-bottom:20px;
			width:100%;
			border-top:1px solid rgb(204, 204, 204);
			text-align:center;
		}
		.game .ipt-txt{
			margin-top:5px;
			vertical-align:middle;
		}
		.game .btn.start{
			margin:20px auto 10px;
			background-color:rgb(64, 201, 64);
			color:white;
		}
		.game .btn.congrat{
			margin-top:5px;
			display:inline-block;
			vertical-align:middle;
			background-color:rgb(255, 64, 0);
			color:white;
		}
		.game .btn.regret{
			margin-top:5px;
			display:inline-block;
			vertical-align:middle;
			background-color:rgb(128, 128, 128);
			color:white;
		}
		.game .member{
			float:left;
			width:40px;
			height:40px;
			background-color:rgb(255, 234, 132);
			border:1px solid rgb(204, 204, 204);
			text-align:center;
			line-height:40px;
			font-weight:bold;
		}
		.game .member.current{
			background-color:rgb(255, 0, 0);
			color:white;
		}
		.game .member.flash{
			animation:flash 2s forwards;
		}
		@keyframes flash{
			10%{
				background-color:rgb(255, 234, 132);
			}
			20%{
				background-color:rgb(255, 0, 0);
			}
			30%{
				background-color:rgb(255, 234, 132);
			}
			40%{
				background-color:rgb(255, 0, 0);
			}
			50%{
				background-color:rgb(255, 234, 132);
			}
			60%{
				background-color:rgb(255, 0, 0);
			}
			70%{
				background-color:rgb(255, 234, 132);
			}
			80%{
				background-color:rgb(255, 0, 0);
			}
			90%{
				background-color:rgb(255, 234, 132);
			}
			100%{
				background-color:rgb(255, 0, 0);
			}
		}
	</style>
	<body>
		<div class="random">
			<div class="page welcome">
				<div class="title">点名系统</div>
				<a class="btn btn-bg start">开始</a>
				<a class="btn btn-bg setting">设置</a>
				<a class="btn btn-bg exit">退出</a>
				<div class="dialog setting hidden">
					<h1>设置</h1>
					<div>
						<input class="ipt-txt sum" type="text" placeholder="总人数" value="50" />
					</div>
					<div>
						<input class="ipt-txt history" type="text" placeholder="历史记录" />
					</div>
					<a class="btn btn-bd sure">确定</a>
				</div>
			</div>
			<div class="page game hidden">
				<div class="page loader hidden">
					<div class="number"></div>
					<div class="progress">
						<div class="inner"></div>
					</div>
				</div>
				<div class="controller">
					<div class="btn btn-bg start">开始</div>
					<input type="text" class="ipt-txt congrat" />
					<div class="btn btn-bg congrat">恭喜</div>
					<input type="text" class="ipt-txt regret" />
					<div class="btn btn-bg regret">遗憾</div>
				</div>
			</div>
		</div>
		<script>
			function Random(option){
				function random(){
					var sum,
						history,
						index = 0,
						arrMember,
						pageGame = document.querySelector(".page.game"),
						fragment = document.createDocumentFragment();
					function loading(callback){
						var loader = document.querySelector(".loader");
						loader.classList.remove("hidden");
						var progress = 0,
							numberBar = loader.querySelector(".number"),
							progressBar = loader.querySelector(".progress"),
							progressBarWidth = progressBar.offsetWidth,
							progressBarInner = progressBar.querySelector(".inner"),
							timerDelay,
							timerLoad = setInterval(function(){
								numberBar.innerHTML = progress = Math.min(100, progress += Math.ceil(Math.random() * 5));
								progressBarInner.style.width = progressBarWidth / 100 * progress + "px";
								if(progress === 100){
									clearInterval(timerLoad);
									timerDelay = setTimeout(function(){
										clearTimeout(timerDelay);
										loader.classList.add("hidden");
										callback();
									}, 500);
								}
							}, 50);
					}
					function init(){
						var btnStart = pageGame.querySelector(".btn.start"),
							iptCongrat = pageGame.querySelector(".ipt-txt.congrat"),
							btnCongrat = pageGame.querySelector(".btn.congrat"),
							iptRegret = pageGame.querySelector(".ipt-txt.regret"),
							btnRegret = pageGame.querySelector(".btn.regret"),
							value,
							newMember;
						pageGame.classList.remove("hidden");
						arrMember = new Array(sum);
						loading(function(){
							createAllMember();
							pageGame.appendChild(fragment);
							btnStart.onclick = function(){
								start();
							};
							btnCongrat.onclick = function(){
								value = +iptCongrat.value;
								arrMember.filter(function(list){
									return list.index === value;
								}).map(function(list){
									pageGame.removeChild(list.dom);
								});
								arrMember = arrMember.filter(function(list){
									return list.index !== value;
								});
							};
							btnRegret.onclick = function(){
								value = +iptRegret.value;
								newMember = createMember(value);
								pageGame.appendChild(newMember);
								arrMember.push({
									index : value,
									dom : newMember
								});
							};
						});
					}
					function createMember(text){
						var dom = document.createElement("div");
						dom.className = "member";
						dom.innerText = text;
						fragment.appendChild(dom);
						return dom;
					}
					function createAllMember(){
						if(history){
							arrMember = history.map(function(list){
								return {
									index : list,
									dom : createMember(list, fragment)
								};
							});
						}else{
							var i = 0,
								len = arrMember.length;
							for(; i < len; i++){
								arrMember[i] = {
									index : i + 1,
									dom : createMember(i + 1, fragment)
								};
							}
						}
					}
					function start(){
						var duration = 0,
							len = arrMember.length,
							timer;
						function getIndex(index){
							return index < len - 1 ? index + 1 : 0;
						}
						function animation(){
							if(duration < 10){
								timer = setTimeout(function(){
									clearTimeout(timer);
									arrMember[index].dom.classList.remove("current");
									index = getIndex(index);
									arrMember[index].dom.classList.add("current");
									animation();
								}, Math.pow(2, duration += Math.random() / Math.pow(7, Math.sqrt(Math.random()))));
							}else{
								clearTimeout(timer);
								arrMember[index].dom.classList.add("flash");
								arrMember[index].dom.addEventListener("animationend", function clearFlash(){
									this.classList.remove("flash");
									this.removeEventListener("animationend", clearFlash);
								});
							}
						}
						animation();
					}
					return {
						init : function(option){
							sum = option.sum;
							history = option.history;
							init();
						}
					};
				}
				function init(){
					var container = document.querySelector(".random"),
						pageWelcome = container.querySelector(".page.welcome"),
						btnStart = container.querySelector(".btn.start"),
						btnSetting = container.querySelector(".btn.setting"),
						btnExit = container.querySelector(".btn.exit"),
						dialogSetting = container.querySelector(".dialog.setting"),
						btnSure = container.querySelector(".btn.sure"),
						iptSum = container.querySelector(".sum"),
						iptHistory = container.querySelector(".history"),
						sum = +iptSum.value,
						history,
						validHistory;
					btnStart.onclick = function(){
						pageWelcome.classList.add("hidden");
						random().init({
							sum : sum,
							history : history
						});
					};
					btnSetting.onclick = function(){
						dialogSetting.classList.toggle("hidden");
					};
					btnExit.onclick = function(){
						document.body.removeChild(container);
					};
					btnSure.onclick = function(){
						sum = +iptSum.value;
						validHistory = iptHistory.value.replace(/\s/g, "");
						history = validHistory ? validHistory.split(",").map(function(list){
							return +list;
						}) : undefined;
						dialogSetting.classList.add("hidden");
					};
				}
				init();
			}
			Random();
		</script>
	</body>
</html>